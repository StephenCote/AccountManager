<service.config>
	<services>
		<service name = "MessageService">
			<methods>
				<method>
					<name>postMessage</name>
					<parameters>
						<parameter>
							<name>message</name>
							<type>org.cote.beans.MessageBean</type>
						</parameter>
					</parameters>
				</method>
				<method>
					<name>postDataMessage</name>
					<parameters>
						<parameter>
							<name>message</name>
							<type>org.cote.beans.DataBean</type>
						</parameter>
					</parameters>
				</method>
				<method>
					<name>getDataMessage</name>
				</method>				
			</methods>
		</service>
		<service name = "TestResources">
			<methods>
				<method>
					<name>debug</name>
					<parameters>
						<parameter>
							<name>message</name>
							<type>org.cote.beans.MessageBean</type>
						</parameter>
					</parameters>
				</method>			
			</methods>
		</service>
